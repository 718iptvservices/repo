<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<addons>
<addon id="repository.RT4" name="RT4" version="1.0.1" provider-name="Tom718">
  <requires>
    <import addon="xbmc.addon" version="12.0.0" />
  </requires>
  <extension point="xbmc.addon.repository" name="RT4 repository">
    <info compressed="false">https://raw.githubusercontent.com/718iptvservices/repo/master/addons.xml</info>
    <checksum>https://raw.githubusercontent.com/718iptvservices/repo/master/addons.xml.md5</checksum>
    <datadir zip="true">https://raw.githubusercontent.com/718iptvservices/zips/master/addons/</datadir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>718 IPTV Repository!</summary>
    <description></description>
    <disclaimer></disclaimer>
    <platform>all</platform>
  </extension>
</addon>
<addon id="plugin.video.718iptvserver"
       name="718 IPTV SERVER"
       version="4.0.1"
       provider-name="Tom718">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.requests" />
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">718 IPTV SERVER</summary>
    <language>en</language>
    <description lang="en">718 IPTV SERVER official addon - Supplier of Quality Streams</description>
     <platform>all</platform>
  </extension>
</addon>
</addons>
